rt default {\r\n        name: \"Home\",\r\n        metaInfo: {\r\n            title: 'Home',\r\n\r\n        },\r\n        data: function () {\r\n            return{\r\n                txt: [\"Hi!\", \"I'm Jenny.\",\"A junior web developer and enthusiast!\"],\r\n                age : ''\r\n            }\r\n        },\r\n\r\n        methods:{\r\n            calcAge: function(){\r\n                let dob = '19990622';\r\n                let year = Number(dob.substr(0, 4));\r\n                let month = Number(dob.substr(4, 2)) - 1;\r\n                let day = Number(dob.substr(6, 2));\r\n                let today = new Date();\r\n                this.age = today.getFullYear() - year;\r\n                if (today.getMonth() < month || (today.getMonth() == month && today.getDate() < day)) {\r\n                    this.age--;\r\n                }\r\n            },\r\n            startTypeWriter: function () {\r\n                let typeWriter = document.getElementById('typeWriter');\r\n                let i;\r\n                let j;\r\n\r\n                for(i = 0 ; i < this.txt.length; i++){\r\n                    for(j = 0; j < this.txt[i].length; j++){\r\n                        let myChar = this.txt[i].charAt(j);\r\n                        typeWriter.innerHTML += myChar;\r\n                    }\r\n                    typeWriter.innerHTML += \"</br>\";\r\n                }\r\n            },\r\n        },\r\n\r\n        mounted() {\r\n            this.startTypeWriter();\r\n            this.calcAge();\r\n        }\r\n\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n    #home{\r\n        background-color: #0f0f0f;\r\n        padding: 50px 0px;\r\n        color: white;\r\n    }\r\n\r\n    #home h2{\r\n        text-align: center;\r\n    }\r\n\r\n    #prof_img_container{\r\n        text-align: center;\r\n        padding-top: 20px;\r\n    }\r\n    #prof_img{\r\n        width: 30%;\r\n        height: auto;\r\n        border-radius: 50%;\r\n\r\n    }\r\n\r\n    .about-me{\